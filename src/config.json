{
    "data": {
        "geno_file": "data/Wheat1k.recode.vcf",
        "pheno_file": "data/wheat1k.pheno.txt",
        "output_dir": "output"
    },
    "preprocessing": {
        "maf_threshold": 0.05,
        "missing_threshold": 0.1,
        "gwas_p_threshold": 0.05,
        "top_n_snps": 1000,
        "top_n_snps_grid": [100, 1000, 3000,5000,7000]
    },
    "training": {
        "random_state": 42,
        "n_jobs": 32
    },
    "models": {
        "RandomForest": {
            "class": "RandomForestModel",
            "task_type": "regression",
            "n_estimators": 100,
            "max_depth": 10,
            "min_samples_split": 2,
            "max_features": "sqrt",
            "random_state": 42,
            "n_jobs": 32,
            "param_grid": {
                "n_estimators": [100, 200],
                "max_depth": [5, 10, 15, 20],
                "min_samples_split": [2, 5],
                "max_features": ["sqrt"]
            }
        },
        "XGBoost": {
            "class": "XGBoostModel",
            "task_type": "regression",
            "n_estimators": 1000,
            "max_depth": 6,
            "learning_rate": 0.1,
            "subsample": 0.8,
            "colsample_bytree": 0.8,
            "random_state": 42,
            "n_jobs": 32,
            "early_stopping_rounds": 20,
            "param_grid": {
                "max_depth": [6, 8],
                "learning_rate": [0.1]
            }
        },
        "LightGBM": {
            "class": "LightGBMModel",
            "task_type": "regression",
            "n_estimators": 1000,
            "max_depth": 3,
            "learning_rate": 0.1,
            "subsample": 0.8,
            "colsample_bytree": 0.8,
            "min_child_samples": 10,
            "min_child_weight": 0.001,
            "min_split_gain": 0.0,
            "reg_alpha": 0.1,
            "reg_lambda": 0.1,
            "verbose": -1,
            "random_state": 42,
            "n_jobs": 32,
            "early_stopping_rounds": 20,
            "param_grid": {
                "max_depth": [3, 5],
                "learning_rate": [0.1]
            }
        },
        "Lasso": {
            "class": "LassoModel",
            "task_type": "regression",
            "alpha": 0.01,
            "max_iter": 2000,
            "tol": 1e-4,
            "random_state": 42,
            "param_grid": {
                "alpha": [0.001, 0.01, 0.1, 1.0]
            }
        },
        "CNN": {
            "class": "CNNModel",
            "task_type": "regression",
            "hidden_sizes": [256, 128, 64],
            "dropout_rate": 0.3,
            "learning_rate": 0.0005,
            "batch_size": 64,
            "epochs": 300,
            "early_stopping_patience": 30,
            "weight_decay": 1e-4,
            "param_grid": {
                "hidden_sizes": [
                    [128, 64],
                    [256, 128, 64],
                    [512, 256, 128, 64]
                ],
                "dropout_rate": [0.2, 0.3, 0.4],
                "learning_rate": [0.0001, 0.0005, 0.001],
                "batch_size": [32, 64, 128],
                "weight_decay": [1e-5, 1e-4, 1e-3]
            }
        },
        "Ensemble": {
            "class": "EnsembleModel",
            "task_type": "regression",
            "cv": 5,
            "random_state": 42,
            "models_config": {
                "elasticnet": {
                    "l1_ratio": [0.1, 0.5, 0.7, 0.9, 0.95, 0.99, 1],
                    "alphas": [0.001, 0.01, 0.1, 1.0, 10.0],
                    "max_iter": 2000,
                    "random_state": 42
                },
                "svr": {
                    "kernel": "rbf",
                    "C": 1.0,
                    "gamma": "scale",
                    "epsilon": 0.1
                },
                "lightgbm": {
                    "n_estimators": 100,
                    "max_depth": 3,
                    "learning_rate": 0.1,
                    "subsample": 0.8,
                    "colsample_bytree": 0.8,
                    "min_child_samples": 10,
                    "reg_alpha": 0.1,
                    "reg_lambda": 0.1,
                    "random_state": 42,
                    "verbose": -1
                },
                "xgboost": {
                    "n_estimators": 100,
                    "max_depth": 6,
                    "learning_rate": 0.1,
                    "subsample": 0.8,
                    "colsample_bytree": 0.8,
                    "random_state": 42
                },
                "meta_model": {
                    "alphas": [0.1, 1.0, 10.0, 100.0]
                }
            },
            "param_grid": {
                "cv": [3, 5, 7],
                "models_config__elasticnet__alphas": [
                    [0.001, 0.01, 0.1, 1.0],
                    [0.01, 0.1, 1.0, 10.0]
                ],
                "models_config__svr__C": [0.1, 1.0, 10.0],
                "models_config__svr__kernel": ["rbf", "linear"],
                "models_config__lightgbm__max_depth": [3, 5, 7],
                "models_config__lightgbm__learning_rate": [0.05, 0.1, 0.2],
                "models_config__xgboost__max_depth": [4, 6, 8],
                "models_config__xgboost__learning_rate": [0.05, 0.1, 0.2],
                "models_config__meta_model__alphas": [
                    [0.1, 1.0, 10.0],
                    [1.0, 10.0, 100.0]
                ]
            }
        }
    }
} 